<div class="container" >

    <%= form_for @importation, url: {action: "create"}, class:"btn btn-lg btn-primary" do |f| %>
  <%# 
    Hack around Rails 5 reversion, see:
    https://github.com/rails/rails/issues/17947#issuecomment-227581029
  %>
  <%= hidden_field_tag 'importation[file]' %>
 
    <%= f.file_field :file %><br />

  <%= f.submit ("send"), class:"btn btn-primary" %>
<% end %>

</div>


